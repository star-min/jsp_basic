	try{
		Class.forName("org.mariadb.jdbc.Driver");
		conn = DriverManager.getConnection("jdbc:mariadb://localhost://3308/company", "root", "1234");
		sql = "";
		pstmt = conn.prepareStatement(sql);
		rs = pstmt.executeQuery();
		cnt = pstmt.executeUpdate();
