<%@ page import="java.sql.*"  %>
	sql = "";
		pstmt = conn.prepareStatement(sql);
		rs = pstmt.executeQuery();
		cnt = pstmt.executeUpdate();
	}catch(Exception e){
		e.printStackTrace();
	} finally {
		try{
			rs.close();
			pstmt.close();
			conn.close();
		}catch(Exception e){
			e.printStackTrace();
		}
	}