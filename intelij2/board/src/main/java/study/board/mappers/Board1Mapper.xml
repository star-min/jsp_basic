<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="study.board.mappers.Board1Mapper">
    <select id="boardCount" resultType="int" >
        SELECT count(seq) AS cnt FROM board1
    </select>
    <select id="getList" resultType="study.board.domain.Board1">
        SELECT
        *
        FROM board1
    </select>
</mapper>